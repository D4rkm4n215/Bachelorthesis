
type Issue{
    iid : ID!
    title : String
    createdAt : String
    state : String
    stateReason : String
    assignees : [Person]
    project : Project
}

extend type Query{
    issues : [Issue]
    issue(id: ID!): Issue
}

input IssueInput {
    title: String!
    state : String
    stateReason : String
    prid: ID!
}

type Mutation {
    createIssue(input: IssueInput): Issue
}